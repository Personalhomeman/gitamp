{
  "name": "ekinhbayar/gitamp",
  "description": "listen ambient music generated by events across github",
  "keywords": [
    "audio",
    "events",
    "github",
    "async",
    "non-blocking"
  ],
  "license": "proprietary",
  "authors": [
    {
      "name": "Ekin H. Bayar",
      "email": "ekin@coproductivity.com",
      "role": "Lead Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7",
    "daverandom/exceptional-json": "^1",
    "nikic/fast-route": "^1",
    "rdlowrey/auryn": "^1",
    "amphp/aerys": "^0.4.4",
    "amphp/artax": "^2",
    "amphp/redis": "^0.2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "ekinhbayar\\GitAmp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ekinhbayar\\GitAmpTests\\": "tests/"
    }
  }
}
